<root>
	<global>
		<namespace value="br.com.project.ui.sessions.session1" />
		<carneiro value="marcelão miranda carneiro" />
	</global>
	
	<base>
		<dependencies>
			<item id="movieclipbase" value="{swfPath}session1.swf" type="movieclip" version="1"/>
